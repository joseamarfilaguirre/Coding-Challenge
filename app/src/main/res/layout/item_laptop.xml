<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/conteiner_item_laptop"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="?attr/listPreferredItemHeight"
    android:orientation="vertical"
    android:paddingBottom="8dp"
    android:paddingTop="8dp">
    <ImageView
        android:id="@+id/image_laptop"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_centerVertical="true"/>
    <TextView
        android:id="@+id/title_laptop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?attr/textAppearanceListItem"
        android:layout_marginLeft="60dp"
        android:text="dasdsadsa"
        />
    <TextView
        android:id="@+id/description_laptop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/title_laptop"
        android:layout_below="@id/title_laptop"
        android:text="dasdsadsa"
        />
</RelativeLayout>
